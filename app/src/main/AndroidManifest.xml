<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rittmann.passwordnotify">

    <application
        android:name=".ui.base.CustomApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.login.LoginActivity"></activity>
        <activity android:name=".ui.listpasswords.ListPasswordsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.managerpassword.ManagerPasswordActivity" />
        <activity android:name=".ui.generatepassword.GeneratePasswordActivity" />
    </application>

</manifest>